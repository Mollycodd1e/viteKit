{
  "name": "stone-kit",
  "description": "uikit for redesign",
  "private": false,
  "version": "0.0.833",
  "author": "Mollycodd1e",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "clean:lib": "node scripts/cleanLib.js",
    "copy:src": "node scripts/copySrcToLib.js",
    "prepare": "npm run clean:lib && npm run copy:src",
    "build": "npm run prepare && tsc && vite build",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "version-update": "node scripts/versionUpdate.js",
    "commit-and-push": "node scripts/commitAndPush.js",
    "release": "npm run build && npm run version-update && npm run commit-and-push && npm publish --tag latest"
  },
  "dependencies": {
    "@react-input/mask": "^1.2.5",
    "react-imask": "^7.6.1"
  },
  "peerDependencies": {
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.2",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-input-mask": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "c8": "^9.1.0",
    "classnames": "^2.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "glob": "^10.3.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.2",
    "react-router-dom": "^6.26.0",
    "sass": "^1.77.2",
    "svgo": "^4.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mollycodd1e/viteKit.git"
